<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical">

	<include
		android:id="@+id/result_setinclude"
		layout="@layout/toolbar"/>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:weightSum="100"
		android:layout_marginTop="18dp"
		android:padding="10dp">

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Scor 100%"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Credit Hour"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Number Grade"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Letter Grade"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text=" Grade Point"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

	</LinearLayout>

	<ListView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/resultsetListView1"
		android:padding="10dp"
		android:dividerHeight="8dp"/>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Scor 100%"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Credit Hour"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Number Grade"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Letter Grade"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text=" Grade Point"
			android:layout_weight="20"
			android:padding="15dp"
			android:ems="6"/>

	</LinearLayout>

	<ListView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/resultsetListView2"/>

</LinearLayout>
