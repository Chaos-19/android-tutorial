<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chaos.Gpacl"
    android:versionCode="214"
    android:versionName="2.1.4" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.chaos.Gpacl.AIDEApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:icon="@drawable/ic_m"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.chaos.Gpacl.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chaos.Gpacl.MainActivity" />
        <activity android:name="com.chaos.Gpacl.InputeHandler" />
        <activity android:name="com.chaos.Gpacl.Result_Activity" />
        <activity android:name="com.chaos.Gpacl.DebugActivity" />
    </application>

</manifest>