<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chaos.Custombttton"
    android:versionCode="214"
    android:versionName="2.1.4" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="28" />

    <application
        android:name="com.chaos.Custombttton.AIDEApplication"
        android:allowBackup="true"
        android:icon="@drawable/light"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.chaos.Custombttton.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chaos.Custombttton.Second_Activity" />
    </application>

</manifest>